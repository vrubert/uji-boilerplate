Ext.define('{{applicationName}}.model.{{modelName}}',
{
    extend : 'Ext.ux.uji.data.Model',

    fields : [
    {
        name : 'id',
        type : 'number'
    },
    {{#each properties}}
    {
        name : '{{this.name}}',
        type : '{{this.type}}',
        useNull : {{#if this.required}}false{{else}}true{{/if}}
    },
    {{/each}}
    ]
});